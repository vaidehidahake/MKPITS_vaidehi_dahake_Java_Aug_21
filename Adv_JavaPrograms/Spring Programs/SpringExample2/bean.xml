<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<bean id="emp" class="com.example.springex2.Employee">
    <property name="id" value="13"></property>
    <property name="name" value="vaidehi"></property>
    <!--when object is initialized the associated data member will get value inserted through object by using setter method-->
</bean>
</beans>